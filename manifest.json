{
  "name": "kaX",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "scripting",
    "tabs",
    "webNavigation"
  ],
  "background": {
    "service_worker": "worker.js"
  },
  "action": {},
  "host_permissions": [
    "https://twitter.com/*",
    "https://x.com/*"
  ],
  "icons": {
    "16": "favicon/favicon16.png",
    "32": "favicon/favicon32.png",
    "48": "favicon/favicon48.png",
    "128": "favicon/favicon128.png"
  }
}
